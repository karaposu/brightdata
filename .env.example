# BrightData SDK Environment Variables
# Copy this file to .env and fill in your actual values

# Main BrightData API Token (required for specialized scrapers)
BRIGHTDATA_TOKEN=your_brightdata_api_token_here

# Web Unlocker Configuration (required for Web Unlocker)
BRIGHTDATA_WEBUNLOCKER_BEARER=your_web_unlocker_bearer_token_here
BRIGHTDATA_WEBUNLOCKER_APP_ZONE_STRING=your_web_unlocker_zone_string_here

# BrowserAPI Configuration 
BRIGHTDATA_BROWSERAPI_USERNAME
BRIGHTDATA_BROWSERAPI_PASSWORD

